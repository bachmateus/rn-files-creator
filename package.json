{
  "name": "@bachmateus/rn-files-creator",
  "version": "2.0.0",
  "description": "rn-files-creator make easier to create new React Native components files. It provides a basic file structure for your components with StyleSheet or StyledComponent.",
  "main": "src/main.js",
  "bin": {
    "@bachmateus/rn-files-creator": "bin/rn",
    "rn": "bin/rn"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "npm run build && rn",
    "build": "npx tsc -p tsconfig-build.json && npm link",
    "test": "jest"
  },
  "keywords": [
    "cli",
    "rn"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "esm": "^3.2.25"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.6",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.8",
    "typescript": "^4.7.4"
  }
}
